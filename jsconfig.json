{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2016",
		"jsx": "preserve",
		"baseUrl": ".",
		"paths": {
			"App": ["./src/App"],
			"App/*": ["./src/App/*"],
			"Components": ["./src/Components"],
			"Components/*": ["./src/Components/*"],
			"Routes": ["./src/Routes"],
			"Routes/*": ["./src/Routes/*"],
			"Util": ["./src/Util"],
			"Util/*": ["./src/Util/*"]
		}
	},
	"exclude": ["node_modules", "**/node_modules/*", "public", "**/public/*"]
}
